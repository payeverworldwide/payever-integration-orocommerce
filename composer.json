{
    "name": "payever/payever-integration-orocommerce",
    "description": "Payever Integration for OroCommerce",
    "keywords": ["payments","payever","e-commerce"],
    "homepage": "https://github.com/payeverworldwide/payever-integration-orocommerce",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Iskusnyh",
            "email": "oleg@iskusnyh.pro",
            "homepage": "https://getpayever.com/",
            "role": "Plugin Developer"
        }
    ],
    "require": {
        "php":"~8.3.0",
        "ext-zip": "*",
        "oro/commerce": "6.0.*",
        "payever/core-sdk-php": "1.1.6",
        "payever/plugins-sdk-php": "1.2.0",
        "payever/payments-sdk-php": "1.2.4",
        "payever/products-sdk-php": "1.1.0"
    },
    "suggest": {
        "ext-bcmath": "If you want to use bcmath functions"
    },
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.orocrm.com"
        }
    },
    "autoload": {
        "psr-4": {"Payever\\Bundle\\PaymentBundle\\": "src"}
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
